{
	"name": "react-ikonate",
	"version": "2.0.0",
	"description": "React version of fully customisable, accessible, well-optimised vector icons",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"es2015": "dist/es/index.js",
	"types": "dist/es/index.d.ts",
	"sideEffects": false,
	"keywords": [
		"icons",
		"outline",
		"customisable",
		"icon system",
		"generator",
		"accessibility"
	],
	"files": [
		"/dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/danielkov/react-ikonate.git"
	},
	"author": "Daniel Kovacs <kovacsemod@gmail.com>",
	"scripts": {
		"generate": "node ../../scripts/generateReact.js",
		"build": "concurrently \"yarn build:es6\" \"yarn build:esm\" \"yarn build:cjs\"",
		"build:es6": "tsc --module es6 --target es6 --outDir dist/es",
		"build:esm": "tsc --module es6 --target es5 --outDir dist/esm",
		"build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
		"test": "jest",
		"test:coverage": "npm test -- --coverage",
		"test:badges": "npm run test:coverage  && jest-coverage-badges"
	},
	"license": "MIT",
	"peerDependencies": {
		"react": "16.8.6",
		"react-dom": "16.8.6"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"\\.(ts|tsx)$": "ts-jest"
		},
		"setupFiles": [
			"raf/polyfill"
		],
		"testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
		"setupFilesAfterEnv": [
			"<rootDir>setupTests.ts"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"coverageReporters": [
			"json-summary",
			"text",
			"lcov"
		]
	}
}
