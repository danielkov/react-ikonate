{
  "name": "react-ikonate",
  "version": "1.0.0",
  "description": "React version of fully customisable, accessible, well-optimised vector icons",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "keywords": [
    "icons",
    "outline",
    "customisable",
    "icon system",
    "generator",
    "accessibility"
  ],
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkov/react-ikonate.git"
  },
  "scripts": {
    "generate": "node scripts/generateReact.js",
    "build": "rollup -c",
    "test": "jest"
  },
  "author": "Mikolaj Dobrucki <tease@mikolajdobrucki.com>",
  "maintainers": [
    "Daniel Kovacs <kovacsemod@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@svgr/core": "^4.2.0",
    "@svgr/plugin-jsx": "^4.2.0",
    "@svgr/plugin-prettier": "^4.2.0",
    "@svgr/plugin-svgo": "^4.2.0",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.14",
    "change-case": "^3.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.10.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4",
    "typings": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
